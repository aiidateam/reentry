{
    "name": "reentry",
    "version": "1.3.2",
    "author": "Rico Haeuselmann",
    "license": "MIT License",
    "url": "https://github.com/dropd/reentry",
    "description": "A plugin manager based on setuptools entry points mechanism",
    "entry_points": {
        "distutils.setup_keywords": [
            "reentry_register = reentry.hooks:register_dist"
        ],
        "console_scripts": [
            "reentry = reentry.cli:reentry"
        ],
        "test_entry_points": [
            "test = reentry.cli:reentry"
        ]
    },
    "classifiers": [
        "License :: OSI Approved :: MIT License",
        "Programming Language :: Python :: 2",
        "Programming Language :: Python :: 3",
        "Development Status :: 5 - Production/Stable",
        "Environment :: Plugins",
        "Intended Audience :: Developers",
        "Topic :: Software Development"
    ],
    "install_requires": [
        "setuptools >= 36.2",
        "click",
        "six",
        "pathlib2; python_version < '3.5'"
    ],
    "extras_require": {
        "tests": [
            "pytest",
            "coverage",
            "pytest-cov",
            "wheel",
            "tox"
        ],
        "pre-commit": [
            "pre-commit~=2.2",
            "pylint~=2.6.0",
            "flake8",
            "tox",
            "packaging"
        ]
    }
}
