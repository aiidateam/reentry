{
    "name": "reentry",
    "version": "1.3.0",
    "author": "Rico Haeuselmann",
    "license": "MIT License",
    "description": "A plugin manager based on setuptools entry points mechanism",
    "entry_points": {
        "distutils.setup_keywords": [
            "reentry_register = reentry.hooks:register_dist"
        ],
        "console_scripts": [
            "reentry = reentry.cli:reentry"
        ],
        "test_entry_points": [
            "test = reentry.cli:reentry"
        ]
    },
    "classifiers": [
        "License :: OSI Approved :: MIT License",
        "Programming Language :: Python :: 2",
        "Programming Language :: Python :: 3",
        "Development Status :: 5 - Production/Stable",
        "Environment :: Plugins",
        "Intended Audience :: Developers",
        "Topic :: Software Development"
    ],
    "install_requires": [
        "setuptools >= 36.2",
        "click",
        "six",
        "pathlib2; python_version < '3.5'"
    ],
    "extras_require": {
        "dev": [
            "pre-commit==1.14.4",
            "prospector==1.1.5",
            "pylint==1.9.4; python_version<'3.0'",
            "pylint==2.2.2; python_version>='3.0'",
            "astroid==2.1.0; python_version>='3.0'",
            "yapf==0.27.0",
            "flake8",
            "pytest",
            "coverage",
            "pytest-cov",
            "tox",
            "packaging"
        ]
    }
}
